{
  "name": "nexus-reussite-app",
  "version": "0.2.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && node scripts/copy-public-assets.js",
    "build:base": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --projects jest.config.unit.js jest.config.integration.js",
    "test:list": "jest --projects jest.config.unit.js jest.config.integration.js --listTests",
    "test:unit": "jest --config jest.config.unit.js",
    "test:integration": "jest --config jest.config.integration.js",
    "test:watch": "jest --projects jest.config.unit.js jest.config.integration.js --watch",
    "test:coverage": "jest --projects jest.config.unit.js jest.config.integration.js --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:test": "node test-db-connection.js",
    "check-config": "node scripts/check-config.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "dev:web": "next dev -p 3000",
    "dev:api": "PYTHONPATH=apps/api:apps/api/apps/api apps/api/.venv/bin/uvicorn apps.api.app.main:app --reload --port 8000",
    "dev:all": "npx concurrently -k -n WEB,API \"npm run dev:web\" \"npm run dev:api\"",
    "db:migrate:py": "apps/api/.venv/bin/alembic -c apps/api/db/alembic.ini upgrade head"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.5",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.536.0",
    "next": "^14.2.32",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.10",
    "openai": "^4.104.0",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.0",
    "sharp": "^0.33.5",
    "tsx": "^4.20.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prisma": "^6.13.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
