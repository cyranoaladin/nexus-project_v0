{
  "name": "nexus-reussite-app",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "zenflow": ".zenflow/cli/index.ts"
  },
  "scripts": {
    "zenflow": "tsx .zenflow/cli/index.ts",
    "dev": "next dev",
    "build": "next build && node scripts/copy-public-assets.js",
    "build:base": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest --projects jest.config.unit.js jest.config.integration.js --runInBand",
    "test:unit": "NODE_ENV=development jest --config jest.config.unit.js",
    "test:unit:watch": "jest --config jest.config.unit.js --watch",
    "test:integration": "jest --config jest.config.integration.js --runInBand",
    "test:integration:with-db": "bash -lc \"if command -v docker >/dev/null 2>&1 && docker info >/dev/null 2>&1; then docker compose -f docker-compose.test.yml up -d; else echo 'Docker not available, skipping test DB startup'; fi\" && npm run test:integration",
    "test:integration:watch": "jest --config jest.config.integration.js --watch",
    "test:watch": "jest --projects jest.config.unit.js jest.config.integration.js --watch",
    "test:coverage": "jest --projects jest.config.unit.js jest.config.integration.js --coverage --runInBand",
    "test:e2e": "SKIP_MIDDLEWARE=true playwright test",
    "test:e2e:ui": "SKIP_MIDDLEWARE=true playwright test --ui",
    "test:e2e:debug": "SKIP_MIDDLEWARE=true playwright test --debug",
    "test:e2e:headed": "SKIP_MIDDLEWARE=true playwright test --headed",
    "test:e2e:chromium": "SKIP_MIDDLEWARE=true playwright test --project=chromium",
    "test:e2e:setup": "bash scripts/setup-e2e-db.sh",
    "test:e2e:seed:parent": "tsx scripts/seed-parent-dashboard-e2e.ts",
    "test:e2e:teardown": "bash scripts/teardown-e2e-db.sh",
    "audit:contrast": "node scripts/audit-contrast.mjs",
    "test:e2e:full": "bash -lc \"node -e \\\"const { chromium } = require('playwright'); chromium.launch({ headless: true, args: ['--no-sandbox'] }).then(b => b.close()).catch(() => process.exit(1));\\\"; if [ $? -ne 0 ]; then export SKIP_CHROMIUM=1; fi; npm run test:e2e:setup && SKIP_CHROMIUM=\\${SKIP_CHROMIUM} npm run test:e2e && npm run test:e2e:teardown\"",
    "test:all": "npm run test:unit && npm run test:integration:with-db && npm run test:e2e:full",
    "test:ci": "npm run test:unit -- --coverage --maxWorkers=2 && npm run test:integration",
    "test:performance": "node --expose-gc --max-old-space-size=4096 node_modules/.bin/jest --config jest.config.performance.js",
    "test:performance:watch": "node --expose-gc --max-old-space-size=4096 node_modules/.bin/jest --config jest.config.performance.js --watch",
    "test:performance:profile": "node --expose-gc --prof --max-old-space-size=4096 node_modules/.bin/jest --config jest.config.performance.js",
    "test:performance:analyze": "node scripts/analyze-performance.js",
    "test:performance:compare": "node scripts/compare-performance.js",
    "verify": "npm run lint && npm run typecheck && npm run test:all && npm run build",
    "verify:quick": "npm run lint && npm run typecheck && npm run test:unit && npm run test:integration",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "check-config": "node scripts/check-config.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:prod:local:up": "bash scripts/start-prod-local.sh",
    "docker:prod:local:down": "docker compose -f docker-compose.prod.yml -f docker-compose.prod.override.yml down",
    "docker:prod:domain:up": "bash scripts/start-prod-local-domain.sh",
    "docker:prod:domain:down": "docker compose -f docker-compose.prod.yml -f docker-compose.prod.override.yml -f docker-compose.prod.local-domain.override.yml down"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/katex": "^0.16.8",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.15.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/three": "^0.182.0",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "bcryptjs": "^3.0.2",
    "chokidar": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.0",
    "gsap": "^3.14.2",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "katex": "^0.16.28",
    "lucide-react": "^0.536.0",
    "next": "^15.5.11",
    "next-auth": "^4.24.11",
    "nodemailer": "^7.0.13",
    "openai": "^4.104.0",
    "pg": "^8.16.3",
    "pino": "^10.3.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.3",
    "playwright": "^1.58.1",
    "react": "^18.3.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-is": "^19.2.4",
    "react-katex": "^3.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "^3.7.0",
    "sharp": "^0.33.5",
    "sonner": "^2.0.7",
    "tar": "^7.5.7",
    "three": "^0.182.0",
    "tsx": "^4.20.3",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.30",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/tailwindcss": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.35",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^6.13.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "overrides": {
    "@next/eslint-plugin-next": {
      "glob": "^10.5.0"
    }
  }
}
